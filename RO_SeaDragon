		name = ModuleEngineConfigs
		type = ModuleEnginesFX
		modded = false
		configuration = x1 thrust
		CONFIG
		{
			name = x1 thrust
			minThrust = 236.6
			maxThrust = 236.6
		}
		CONFIG
		{
			name = x3 thrust
			minThrust = 709.8
			maxThrust = 709.8
		}
		CONFIG
		{
			name = x5 thrust
			minThrust = 1183
			maxThrust = 1183
		}
	}
	
	
}


