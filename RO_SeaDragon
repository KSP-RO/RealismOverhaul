@PART[RSSDengineStage1]:FOR[RealismOverhaul]
{
	@node_stack_bottom = 0.0, -27, 0.0, 0.0, -1.0, 0.0, 8
	%RSSROConfig = True
	%mass = 465.1
	@MODULE[ModuleEngines*]
	{
		@atmosphereCurve
		{
			@key,0 = 0 260 //guess, correct value for 121s burn and ~1700m/s velocity at staging
			@key,1 = 1 242 //rated sl isp
		}
		@PROPELLANT[LiquidFuel]    //oxidizer:fuel = 2.3:1
		{
			%name = Kerosene
			%ratio = 0.375
		}
		@PROPELLANT[Oxidizer]
		{
			%name = LqdOxygen
			%ratio = 0.625
		}
	}
}
